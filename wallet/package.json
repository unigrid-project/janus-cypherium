{
  "name": "Janus-Cypherium",
  "version": "0.0.19",
  "license": "LGPL-3.0-or-later",
  "author": "The UNIGRID Organization",
  "description": "A modern cryptocurrency wallet based on the JavaScript Electron UI toolkit. Janus open source wallet can be used for multiple different types of blockchains.",
  "scripts": {
    "dev": "electron-webpack dev",
    "compile": "electron-webpack",
    "dist": "yarn compile && electron-builder",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null",
    "release": "yarn dist -p always"
  },
  "dependencies": {
    "@babel/core": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@cypherium/web3c": "^0.10.4",
    "@fortawesome/fontawesome-svg-core": "^1.2.29",
    "@fortawesome/free-brands-svg-icons": "^5.13.1",
    "@fortawesome/free-solid-svg-icons": "^5.13.1",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@sentry/electron": "^2.0.0",
    "@stablelib/ed25519": "^1.0.1",
    "aes256": "^1.0.4",
    "axios": "^0.21.0",
    "bip39": "^3.0.3",
    "cypheriumjs-tx": "^0.10.2",
    "electron-log": "^4.2.2",
    "electron-store": "^6.0.1",
    "electron-updater": "^4.3.5",
    "empty-promise": "^1.2.0",
    "gettext-parser": "^4.0.4",
    "internet-available": "^1.0.0",
    "javascript-terminal": "^1.1.0",
    "jquery": "^3.5.1",
    "jssha": "^3.2.0",
    "less": "^3.11.3",
    "less-loader": "^6.1.3",
    "lodash": "^4.17.15",
    "node-gettext": "^3.0.0",
    "node-json-rpc2": "^1.1.0",
    "node-notifier": "^7.0.1",
    "objects-to-csv": "^1.3.6",
    "portscanner": "^2.2.0",
    "qrcode.react": "^1.0.1",
    "react": "17.0.0",
    "react-dom": "17.0.0",
    "react-dropdown-select": "^4.7.3",
    "react-expand-animated": "^1.0.2",
    "react-flag-kit": "^0.3.1",
    "react-helmet": "^6.1.0",
    "react-highlight": "^0.12.0",
    "react-select": "^3.1.0",
    "react-simple-infinite-loading": "^1.1.0",
    "react-simple-tooltip": "^2.6.2",
    "react-terminal-component": "^1.4.1",
    "react-window": "^1.8.5",
    "ripemd160": "^2.0.2",
    "roboto-fontface": "^0.10.0",
    "source-map-support": "^0.5.19",
    "ssh2": "^0.8.9",
    "styled-components": "^5.1.1",
    "tcp-port-used": "^1.0.1",
    "tweetnacl": "^1.0.3",
    "web3": "^1.3.0",
    "yarn": "^1.22.4"
  },
  "devDependencies": {
    "electron": "^11.2.3",
    "electron-builder": "^22.9.1",
    "electron-webpack": "2.8.2",
    "electron-webpack-eslint": "6.0.0",
    "webpack": "4.43.0"
  },
  "resolutions": {
    "upath": "^1.2.0"
  },
  "build": {
    "appId": "org.unigrid.wallet",
    "mac": {
      "category": "org.unigrid.wallet",
      "extraFiles": [
        {
          "from": "locale",
          "to": "resources/locale",
          "filter": [
            "**/*"
          ]
        },
        {
          "from": "config.json",
          "to": "resources/config.json",
          "filter": [
            "**/*"
          ]
        }
      ]
    },
    "linux": {
      "asar": "true",
      "target": [
        "AppImage"
      ],
      "extraFiles": [
        {
          "from": "../daemon/unigridd",
          "to": "unigridd"
        },
        {
          "from": "locale",
          "to": "resources/locale",
          "filter": [
            "**/*"
          ]
        },
        {
          "from": "config.json",
          "to": "resources/config.json",
          "filter": [
            "**/*"
          ]
        }
      ],
      "category": "science"
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "./build/icon.ico",
      "extraFiles": [
        {
          "from": "../daemon/unigridd.exe",
          "to": "unigridd.exe"
        },
        {
          "from": "locale",
          "to": "resources/locale",
          "filter": [
            "**/*"
          ]
        },
        {
          "from": "config.json",
          "to": "resources/config.json",
          "filter": [
            "**/*"
          ]
        }
      ]
    },
    "nsis": {
      "oneClick": true,
      "allowToChangeInstallationDirectory": false
    },
    "publish": [
      {
        "provider": "github",
        "owner": "unigrid-project",
        "repo": "janus",
        "releaseType": "release"
      }
    ],
    "files": [
      "**/*",
      "logo.png",
      "solid_logo.png",
      "config.json"
    ],
    "copyright": "Copyright (C) 2019-2021 The UNIGRID Organization"
  }
}
